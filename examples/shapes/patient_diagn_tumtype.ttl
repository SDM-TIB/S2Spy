#LCPatients have a Diagnosis and a TumorType. A Diagnosis has a TumorType as well. Thus they should be the same.

@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix iasis: <http://project-iasis.eu/vocab/>.
#<http://project-iasis.eu/TumorType/>.

<http://project-iasis.eu/vocab/>
  rdf:type owl:Ontology ;
  rdfs:label "Test two classes constraint" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Version 1" ;
.

iasis:PatientShape
  rdf:type sh:NodeShape ;
  sh:closed "false"^^xsd:boolean ;
  sh:targetClass iasis:LCPatient ;
  sh:property [
    sh:path iasis:tumorType;
    sh:node iasis:TumorTypeShape;
  ]
.

iasis:DiagnosisShape
  rdf:type sh:NodeShape ;
  sh:closed "false"^^xsd:boolean ;
  sh:targetClass iasis:Diagnosis ;
  sh:property [
    sh:path iasis:hasDiagnosisTumorType;
    sh:node iasis:TumorTypeShape;
  ]
.

iasis:TumorTypeShape
  rdf:type sh:NodeShape;
  sh:property [
    sh:path iasis:tumorTypeLabel_ENG ;
    sh:datatype xsd:string ;
  ]
.

