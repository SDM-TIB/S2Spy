@prefix owl: <http://www.w3.org/2002/07/owl#> .
@prefix sh: <http://www.w3.org/ns/shacl#> .
@prefix rdf: <http://www.w3.org/1999/02/22-rdf-syntax-ns#> .
@prefix rdfs: <http://www.w3.org/2000/01/rdf-schema#> .
@prefix xsd: <http://www.w3.org/2001/XMLSchema#> .
@prefix ex: <http://datashapes.org/sh/tests/core/complex/example.test#> .
@prefix iasis: <http://project-iasis.eu/vocab/>.

<http://datashapes.org/sh/tests/core/complex/personexample.test>
  rdf:type owl:Ontology ;
  rdfs:label "Test example" ;
  owl:imports <http://datashapes.org/dash> ;
  owl:versionInfo "Version 1" ;
.

ex:Example
  rdf:type sh:NodeShape ;
  sh:closed "false"^^xsd:boolean ;
  sh:ignoredProperties (
      rdf:type
    ) ;
  sh:property _:b1 ;
  sh:property _:b2 ;
  sh:targetClass iasis:LCPatient ;
.

_:b1
  sh:path iasis:numberOfCigarettesPerYear ;
  sh:datatype xsd:string ;
  sh:maxCount 1 ;
  sh:pattern "(?!0|[a-z]|[A-Z])(\\d)+" ;
  sh:message "Number of cigarettes greater than 0, max count 1"
.

_:b2
  sh:path iasis:smoking ;
  sh:hasValue "current" ;
.